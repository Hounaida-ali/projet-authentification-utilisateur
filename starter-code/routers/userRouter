const express = require('express');
const router = express.Router();

const {register,verifyOtp,connectUser,reinisilize,resetPassword} = require("../controllers/userController");

router.post ("/register",register);
router.patch("/verify-email", verifyOtp);
router.post ("/connectUser",connectUser);
router.post("/reinisilize", reinisilize);
router.patch("/resetPassword", resetPassword);




module.exports = router;

